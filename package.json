{
  "name": "backend-refactor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "babel-node ./bin/www.js",
    "dev": "nodemon --exec npm start",
    "lint": "eslint .",
    "build": "webpack",
    "test": "jest --watch",
    "ci-test": "jest --coverage",
    "serve": "pm2 start -f build/index.js --name backend-refactor --watch --ignore-watch='node_modules keys/*'",
    "deploy-setup": "npm install && npm run build && pm2 reload all",
    "deploy": "npm install && npm run build && npm run serve",
    "deploy-down": "pm2 stop backend-refactor",
    "docker-serve": "node build/index.js",
    "migrate": "sh ./prisma_migrate.sh",
    "start:migrate": "npm run migrate && npm run start"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.231.0",
    "@babel/cli": "^7.17.10",
    "@babel/node": "^7.18.5",
    "@google-cloud/translate": "^7.0.4",
    "@prisma/client": "^5.2.0",
    "apn": "^2.2.0",
    "aws-sdk": "^2.1247.0",
    "babel-plugin-root-import": "^6.6.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "es6-promise": "^4.2.8",
    "express": "^4.18.2",
    "iso-639-1": "^2.1.15",
    "isomorphic-fetch": "^3.0.0",
    "joi": "^17.6.0",
    "joi-oid": "^1.1.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pm2": "^5.2.0",
    "randomatic": "^3.1.1",
    "sharp": "^0.32.0",
    "socket.io": "^4.5.3",
    "swagger-ui-express": "^4.5.0",
    "twilio": "^3.84.1",
    "unleash-proxy-client": "^2.4.1",
    "uuid": "^9.0.0",
    "yamljs": "^0.3.0"
  },
  "babel": {
    "retainLines": true,
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@tsconfig/node18": "^18.2.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.19.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-expect-jwt": "^1.0.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.2.0",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
